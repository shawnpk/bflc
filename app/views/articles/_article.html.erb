<tr>
  <td><%= link_to article.title, article %></td>
  <td>Published <%= article.created_at.strftime('%B %-e, %Y') %></td>
  <td><%= article.body %></td>
  <td><%= link_to 'Show', article %></td>
  <td><%= link_to 'Edit', edit_article_path(article) %></td>
  <td><%= link_to 'Delete', article, method: :delete, data: { confirm: "Are you sure you want to delete #{article.title}?" } %></td>
</tr>
